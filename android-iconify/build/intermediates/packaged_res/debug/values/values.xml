<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconImageView">
        <attr format="string" name="iconName"/>
        <attr format="color" name="iconColor"/>
        <attr format="enum" name="iconAnimation">
            <enum name="spin" value="0"/>
            <enum name="pulse" value="1"/>
            <enum name="none" value="2"/>
        </attr>
    </declare-styleable>
</resources>